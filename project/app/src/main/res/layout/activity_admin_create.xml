<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.bank.bank.AdminCreate">

    <Button
        android:id="@+id/create_admin"
        android:layout_width="265dp"
        android:layout_height="73dp"
        android:text="@string/createAdmin"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/create_teller"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="203dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintVertical_bias="0.262" />

    <Button
        android:id="@+id/create_teller"
        android:layout_width="265dp"
        android:layout_height="73dp"
        android:text="@string/createTeller"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="101dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />
</android.support.constraint.ConstraintLayout>
