<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.bank.bank.AdminListTellers"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <ListView
        android:id="@+id/TellerList"
        android:layout_width="385dp"
        android:layout_height="401dp"
        android:layout_marginBottom="-14dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="8dp"
        android:background="@color/colorWhite"
        android:contextClickable="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_editor_absoluteY="110dp">
    </ListView>

    <TextView
        android:id="@+id/TellerText"
        android:layout_width="384dp"
        android:layout_height="109dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/CustomerList"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="1dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="1dp"
        android:background="@color/colorWhite"
        android:fontFamily="casual"
        android:text="@string/tellerinit"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/TellerList"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        />
</android.support.constraint.ConstraintLayout>
